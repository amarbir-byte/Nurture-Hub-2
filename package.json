{
  "name": "nurture-hub",
  "private": true,
  "version": "1.0.0",
  "description": "Proximity-based marketing CRM for real estate agents. 50% cheaper than kvCORE with better results.",
  "type": "module",
  "author": "Nurture Hub Team",
  "keywords": [
    "real-estate",
    "crm",
    "proximity-marketing",
    "sms-campaigns",
    "property-management"
  ],
  "homepage": "https://nurture-hub.vercel.app",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "analyze": "vite-bundle-analyzer",
    "build:analyze": "npm run build && npm run analyze",
    "typecheck": "tsc --noEmit",
    "security:audit": "node scripts/security-audit.js",
    "test:performance": "npm run build && npm run preview --port 4173 &",
    "pre-deploy": "npm run typecheck && npm run lint && npm run security:audit && npm run build"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@stripe/react-stripe-js": "^4.0.2",
    "@stripe/stripe-js": "^7.9.0",
    "@supabase/supabase-js": "^2.57.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@dyad-sh/react-vite-component-tagger": "^0.8.0",
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-pwa": "^1.0.3"
  }
}
